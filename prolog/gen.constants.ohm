Prolog {
  Program = Sexpr+
  Sexpr =
    | "(" Sexpr* ")" -- list
    | "'" Sexpr -- quoted
    | string -- string
    | number -- number
    | Symbol -- symbol

  separator = "(" | ")" | "'" | "," | "@" | "\x60" | ";" | " . "
  vcomma = "â‚“"
  Symbol = symchar+ vcomma?
  symchar = ~vcomma ~separator ~space any
  
  string = dq (~dq any)* dq
  dq = "\""
  number = digit+ vcomma?
  semiColonComment = ";" (~"\n" any)* "\n"
  space += semiColonComment
}


Constants <: Prolog {
  Sexpr += "(" sym<"quote"> CSexpr ")" -- beginconstantlist

  CSexpr = 
    | "(" ")" -- constantnil
    | "(" CSexpr+ ")" -- constantlist
    | string -- constantstring
    | integer -- constantinteger
    | Symbol -- constantsymbol

  integer = number
  sym<s> = s vcomma?
}
